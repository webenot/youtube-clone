mixin videoPlayer(video)
  if video
    .videoPlayer
      video(src=`/${video.fileUrl}`)
      .videoPlayer__controls
        .videoPlayer__column
          span
            i.fas.fa-volume-up
          span 00:00:00 / 00:10:00
        .videoPlayer__column
          span
            i.fas.fa-play
        .videoPlayer__column
          span
            i.fas.fa-expand
